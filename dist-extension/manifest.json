{
  "manifest_version": 3,
  "name": "Tabz - Chrome Edition",
  "version": "1.2.6",
  "description": "Full-featured terminal manager in your browser with xterm.js, tmux integration, and multi-window support",
  "permissions": [
    "storage",
    "contextMenus",
    "tabs",
    "sidePanel",
    "clipboardRead",
    "clipboardWrite",
    "notifications",
    "scripting",
    "activeTab",
    "alarms",
    "debugger",
    "downloads",
    "webRequest",
    "bookmarks",
    "pageCapture"
  ],
  "omnibox": {
    "keyword": "term"
  },
  "host_permissions": [
    "<all_urls>",
    "http://localhost:8129/*",
    "ws://localhost:8129/*",
    "http://127.0.0.1:8129/*",
    "ws://127.0.0.1:8129/*",
    "https://github.com/*",
    "https://gitlab.com/*",
    "http://localhost/*",
    "http://127.0.0.1/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'",
    "sandbox": "sandbox allow-scripts allow-forms allow-popups allow-modals; script-src 'self' 'unsafe-inline' 'unsafe-eval'; child-src 'self';"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "side_panel": {
    "default_path": "sidepanel/sidepanel.html"
  },
  "commands": {
    "_execute_action": {
      "description": "Toggle terminal sidebar"
    },
    "new-tab": {
      "suggested_key": {
        "default": "Alt+T"
      },
      "description": "New terminal tab (default profile)"
    },
    "close-tab": {
      "suggested_key": {
        "default": "Alt+W"
      },
      "description": "Close current terminal tab"
    },
    "paste-selection": {
      "suggested_key": {
        "default": "Alt+V"
      },
      "description": "Paste selected text to terminal"
    },
    "send-to-chat": {
      "suggested_key": {
        "default": "Alt+C"
      },
      "description": "Send selected text to chat"
    },
    "next-tab": {
      "description": "Switch to next terminal tab"
    },
    "prev-tab": {
      "description": "Switch to previous terminal tab"
    },
    "tab-1": {
      "description": "Switch to terminal tab 1"
    },
    "tab-2": {
      "description": "Switch to terminal tab 2"
    },
    "tab-3": {
      "description": "Switch to terminal tab 3"
    },
    "tab-4": {
      "description": "Switch to terminal tab 4"
    },
    "tab-5": {
      "description": "Switch to terminal tab 5"
    },
    "tab-6": {
      "description": "Switch to terminal tab 6"
    },
    "tab-7": {
      "description": "Switch to terminal tab 7"
    },
    "tab-8": {
      "description": "Switch to terminal tab 8"
    },
    "tab-9": {
      "description": "Switch to terminal tab 9"
    }
  },
  "devtools_page": "devtools/devtools.html",
  "content_scripts": [
    {
      "js": [
        "assets/content.ts-DBi0gqX4.js"
      ],
      "matches": [
        "https://github.com/*",
        "https://gitlab.com/*",
        "https://ggprompts.github.io/*",
        "http://localhost/*",
        "http://127.0.0.1/*"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "http://127.0.0.1/*",
        "http://localhost/*",
        "https://ggprompts.github.io/*",
        "https://github.com/*",
        "https://gitlab.com/*"
      ],
      "resources": [
        "spawn-options.json",
        "profiles.json",
        "icons/tabz-logo-light.png",
        "assets/content.ts-DBi0gqX4.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost:8129/*",
      "http://127.0.0.1:8129/*",
      "https://personal-homepage-virid.vercel.app/*"
    ]
  }
}
