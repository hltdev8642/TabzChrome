{
  "name": "tabzchrome-marketplace",
  "owner": {
    "name": "GGPrompts",
    "url": "https://github.com/GGPrompts"
  },
  "metadata": {
    "description": "TabzChrome plugins - terminal management, browser automation, and worker orchestration",
    "version": "2.0.0"
  },
  "plugins": [
    {
      "name": "conductor",
      "source": "./plugins/conductor",
      "description": "Lightweight orchestrator that coordinates worker pipelines - delegates to prompt-writer, planner, spawner, and cleanup plugins",
      "version": "3.0.0",
      "author": {
        "name": "GGPrompts",
        "url": "https://github.com/GGPrompts"
      },
      "homepage": "https://github.com/GGPrompts/TabzChrome",
      "repository": "https://github.com/GGPrompts/TabzChrome",
      "license": "MIT",
      "category": "orchestration",
      "keywords": [
        "orchestration",
        "beads",
        "conductor",
        "worker",
        "pipeline"
      ],
      "strict": true,
      "commands": [
        "./commands/auto.md",
        "./commands/plan-backlog.md",
        "./commands/gate-runner.md",
        "./commands/status.md"
      ],
      "agents": [
        "./agents/precommit-gate.md"
      ],
      "skills": [
        "./skills/automating-browser",
        "./skills/brainstorming",
        "./skills/docs-check",
        "./skills/reviewing-code",
        "./skills/running-tests",
        "./skills/visual-qa"
      ]
    },
    {
      "name": "prompt-writer",
      "source": "./plugins/prompt-writer",
      "description": "Crafts optimized worker prompts from backlog issues using Claude 4 prompt engineering best practices",
      "version": "1.0.0",
      "author": {
        "name": "GGPrompts",
        "url": "https://github.com/GGPrompts"
      },
      "homepage": "https://github.com/GGPrompts/TabzChrome",
      "repository": "https://github.com/GGPrompts/TabzChrome",
      "license": "MIT",
      "category": "prompting",
      "keywords": [
        "prompts",
        "prompt-engineering",
        "workers",
        "beads"
      ],
      "strict": true,
      "commands": [
        "./commands/write.md",
        "./commands/write-all.md"
      ],
      "agents": [],
      "skills": [
        "./skills/prompt-craft"
      ]
    },
    {
      "name": "planner",
      "source": "./plugins/planner",
      "description": "Breaks down features and epics into concrete backlog issues with dependency tracking",
      "version": "1.0.0",
      "author": {
        "name": "GGPrompts",
        "url": "https://github.com/GGPrompts"
      },
      "homepage": "https://github.com/GGPrompts/TabzChrome",
      "repository": "https://github.com/GGPrompts/TabzChrome",
      "license": "MIT",
      "category": "planning",
      "keywords": [
        "planning",
        "decomposition",
        "epics",
        "backlog",
        "beads"
      ],
      "strict": true,
      "commands": [
        "./commands/plan.md"
      ],
      "agents": [],
      "skills": [
        "./skills/breakdown"
      ]
    },
    {
      "name": "spawner",
      "source": "./plugins/spawner",
      "description": "Spawns Claude workers via TabzChrome API with worktree isolation and prepared prompts",
      "version": "1.0.0",
      "author": {
        "name": "GGPrompts",
        "url": "https://github.com/GGPrompts"
      },
      "homepage": "https://github.com/GGPrompts/TabzChrome",
      "repository": "https://github.com/GGPrompts/TabzChrome",
      "license": "MIT",
      "category": "terminals",
      "keywords": [
        "spawn",
        "terminals",
        "worktrees",
        "tabzchrome",
        "workers"
      ],
      "strict": true,
      "commands": [
        "./commands/spawn.md"
      ],
      "agents": [],
      "skills": [
        "./skills/terminals"
      ]
    },
    {
      "name": "cleanup",
      "source": "./plugins/cleanup",
      "description": "Post-work cleanup - merges branches, removes worktrees, syncs beads after worker completion",
      "version": "1.0.0",
      "author": {
        "name": "GGPrompts",
        "url": "https://github.com/GGPrompts"
      },
      "homepage": "https://github.com/GGPrompts/TabzChrome",
      "repository": "https://github.com/GGPrompts/TabzChrome",
      "license": "MIT",
      "category": "cleanup",
      "keywords": [
        "cleanup",
        "merge",
        "worktrees",
        "git"
      ],
      "strict": true,
      "commands": [
        "./commands/done.md"
      ],
      "agents": [],
      "skills": [
        "./skills/merge"
      ]
    }
  ]
}
