{
  "name": "tabzchrome",
  "version": "1.2.2",
  "description": "Windows Terminal-style Chrome extension for managing bash terminals in your browser sidebar",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/GGPrompts/TabzChrome.git"
  },
  "homepage": "https://github.com/GGPrompts/TabzChrome#readme",
  "bugs": {
    "url": "https://github.com/GGPrompts/TabzChrome/issues"
  },
  "keywords": [
    "chrome-extension",
    "tmux",
    "terminal",
    "sidebar",
    "xterm",
    "bash",
    "tabz"
  ],
  "author": "Matt",
  "license": "MIT",
  "scripts": {
    "dev": "vite --config vite.config.extension.ts",
    "build": "tsc --project tsconfig.extension.json && vite build --config vite.config.extension.ts",
    "zip": "cd dist-extension && zip -r ../terminal-tabs-extension.zip .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@xterm/addon-canvas": "^0.7.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-unicode11": "^0.8.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "cmdk": "^1.1.1",
    "immer": "^10.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "remark-gfm": "^4.0.1",
    "three": "^0.182.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.2.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chrome": "^0.1.29",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-resizable": "^3.0.8",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "autoprefixer": "^10.4.22",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "lucide-react": "^0.553.0",
    "patch-package": "^8.0.1",
    "postcss": "^8.5.6",
    "react-resizable": "^3.0.5",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vite-plugin-web-extension": "^4.5.0",
    "vitest": "^4.0.8"
  }
